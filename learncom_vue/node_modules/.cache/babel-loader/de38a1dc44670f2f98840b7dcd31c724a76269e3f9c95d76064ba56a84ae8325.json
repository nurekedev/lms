{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      password2: '',\n      errors: []\n    };\n  },\n  mounted() {\n    document.title = 'Sign up | LMS';\n  },\n  methods: {\n    submitForm() {\n      console.log('sub');\n      this.errors = [];\n      if (this.username === '') {\n        this.errors.push(\"Username is missing\");\n      }\n      if (this.password === '') {\n        this.errors.push(\"Password is missing\");\n      }\n      if (this.password !== this.password2) {\n        this.errors.push(\"Passwords don not match\");\n      }\n      if (!this.errors.length) {\n        const formData = {\n          username: this.username,\n          password: this.password\n        };\n        axios.post('/api/v1/users/', formData).then(response => {\n          this.$router.push('/login');\n        }).catch(error => {\n          if (error.response) {\n            for (const property in error.response.data) {\n              this.errors.push(`${property}: ${error.response.data[property]}`);\n            }\n            console.log(JSON.stringify(error.response.data));\n          } else if (error.message) {\n            this.errors.push('Something went wrong. Please try again');\n            console.log(JSON.stringify(error));\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","password2","errors","mounted","document","title","methods","submitForm","console","log","push","length","formData","post","then","response","$router","catch","error","property","JSON","stringify","message"],"sources":["/Users/nurzhansaktaganov/Desktop/LMS/learncom_vue/src/views/SignUpView.vue"],"sourcesContent":["<template>\n    <div class=\"signup\">\n        <div class=\"hero is-info\">\n            <div class=\"hero-body has-text-centered\">\n                <h1 class=\"title\"> Sign up</h1>\n            </div>\n        </div>\n\n        <section class=\"section\">\n            <div class=\"container\">\n                <div class=\"columns\">\n                    <div class=\"column is-4 is-offset-4\">\n\n                        <form v-on:submit.prevent=\"submitForm\">\n                            <div class=\"notification is-danger\" v-if=\"errors.length\">\n                                <p v-for=\"error in errors\" v-bind:key=\"error\">\n                                    {{ error }}\n                                </p>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>Email</label>\n                                <div class=\"control\">\n                                    <input type=\"email\" class=\"input\" v-model=\"username\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>Password</label>\n                                <div class=\"control\">\n                                    <input type=\"password\" class=\"input\" v-model=\"password\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>Confirm password</label>\n                                <div class=\"control\">\n                                    <input type=\"password\" class=\"input\" v-model=\"password2\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <div class=\"control\">\n                                    <button class=\"button is-dark\">Sign up</button>\n                                </div>\n                            </div>\n                        </form>\n\n\n\n\n                        <hr>\n                        or\n                        <router-link to=\"/log-in\">You already have account</router-link>\n                    </div>\n                </div>\n            </div>\n        </section>\n    </div>\n</template>\n\n\n<script>\nimport axios from 'axios'\n\n\nexport default {\n\n    data() {\n        return {\n            username: '',\n            password: '',\n            password2: '',\n            errors: []\n        }\n    },\n\n    mounted(){\n        document.title = 'Sign up | LMS'\n    },\n\n    methods: {\n        submitForm() {\n            console.log('sub')\n\n            this.errors = []\n\n            if (this.username === '') {\n                this.errors.push(\"Username is missing\")\n            }\n\n            if (this.password === '') {\n                this.errors.push(\"Password is missing\")\n            }\n\n            if (this.password !== this.password2) {\n                this.errors.push(\"Passwords don not match\")\n            }\n\n            if (!this.errors.length) {\n                const formData = {\n                    username: this.username,\n                    password: this.password\n                }\n\n                axios\n                    .post('/api/v1/users/', formData)\n                    .then(response => {\n                        this.$router.push('/login')\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            console.log(JSON.stringify(error))\n                        }\n                    })\n\n            }\n\n\n\n        }\n    }\n}\n\n</script>"],"mappings":";AA+DA,OAAOA,KAAI,MAAO,OAAM;AAGxB,eAAe;EAEXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE;IACZ;EACJ,CAAC;EAEDC,OAAOA,CAAA,EAAE;IACLC,QAAQ,CAACC,KAAI,GAAI,eAAc;EACnC,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACTC,OAAO,CAACC,GAAG,CAAC,KAAK;MAEjB,IAAI,CAACP,MAAK,GAAI,EAAC;MAEf,IAAI,IAAI,CAACH,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACG,MAAM,CAACQ,IAAI,CAAC,qBAAqB;MAC1C;MAEA,IAAI,IAAI,CAACV,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACE,MAAM,CAACQ,IAAI,CAAC,qBAAqB;MAC1C;MAEA,IAAI,IAAI,CAACV,QAAO,KAAM,IAAI,CAACC,SAAS,EAAE;QAClC,IAAI,CAACC,MAAM,CAACQ,IAAI,CAAC,yBAAyB;MAC9C;MAEA,IAAI,CAAC,IAAI,CAACR,MAAM,CAACS,MAAM,EAAE;QACrB,MAAMC,QAAO,GAAI;UACbb,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB;QAEAH,KAAI,CACCgB,IAAI,CAAC,gBAAgB,EAAED,QAAQ,EAC/BE,IAAI,CAACC,QAAO,IAAK;UACd,IAAI,CAACC,OAAO,CAACN,IAAI,CAAC,QAAQ;QAC9B,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;UACZ,IAAIA,KAAK,CAACH,QAAQ,EAAE;YAChB,KAAK,MAAMI,QAAO,IAAKD,KAAK,CAACH,QAAQ,CAACjB,IAAI,EAAE;cACxC,IAAI,CAACI,MAAM,CAACQ,IAAI,CAAE,GAAES,QAAS,KAAID,KAAK,CAACH,QAAQ,CAACjB,IAAI,CAACqB,QAAQ,CAAE,EAAC;YACpE;YAEAX,OAAO,CAACC,GAAG,CAACW,IAAI,CAACC,SAAS,CAACH,KAAK,CAACH,QAAQ,CAACjB,IAAI,CAAC;UACnD,OAAO,IAAIoB,KAAK,CAACI,OAAO,EAAE;YACtB,IAAI,CAACpB,MAAM,CAACQ,IAAI,CAAC,wCAAwC;YACzDF,OAAO,CAACC,GAAG,CAACW,IAAI,CAACC,SAAS,CAACH,KAAK,CAAC;UACrC;QACJ,CAAC;MAET;IAIJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}