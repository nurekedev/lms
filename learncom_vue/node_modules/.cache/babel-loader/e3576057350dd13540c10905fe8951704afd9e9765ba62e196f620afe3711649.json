{"ast":null,"code":"import CourseItemComponent from '@/components/CourseItemComponent.vue';\nimport router from '@/router';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      courses: [],\n      categories: [],\n      activeCategory: {}\n    };\n  },\n  async mounted() {\n    console.log(\"Mounted\");\n    document.title = 'Courses | LMS';\n    await axios.get('api/v1/courses/get-categories/').then(response => {\n      console.log(response.data);\n      this.categories = response.data;\n    });\n    this.getCourses;\n  },\n  components: {\n    router,\n    CourseItemComponent\n  },\n  methods: {\n    setActiveCategory(category) {\n      this.activeCategory = category;\n      this.getCourses();\n    },\n    getCourses() {\n      let url = '/api/v1/courses/';\n      if (this.activeCategory) {\n        url += '?category_id=' + this.activeCategory.id;\n      }\n      axios.get(url).then(response => {\n        console.log(response.data);\n        this.courses = response.data;\n      });\n    }\n  }\n};","map":{"version":3,"names":["CourseItemComponent","router","axios","data","courses","categories","activeCategory","mounted","console","log","document","title","get","then","response","getCourses","components","methods","setActiveCategory","category","url","id"],"sources":["/Users/nurzhansaktaganov/Desktop/LMS/learncom_vue/src/views/CoursesView.vue"],"sourcesContent":["<template>\n    <div class=\"courses\">\n        <div class=\"hero is-info\">\n            <div class=\"hero-body has-text-centered\">\n                <h1 class=\"title\"> Courses </h1>\n            </div>\n        </div>\n\n        <section class=\"section\">\n            <div class=\"container\">\n                <div class=\"columns\">\n\n                    <div class=\"column is-2\">\n                        <aside class=\"menu\">\n                            <p class=\"menu-label\">\n                                Categories\n                            </p>\n                            <ul class=\"menu-list\">\n                                <li>\n                                    <a v-bind:class=\"{ 'is-active': !activeCategory }\" @click=\"setActiveCategory(null)\">\n                                        All courses</a>\n\n                                </li>\n                                <li v-for=\"category in categories\" v-bind:key=\"category.id\"\n                                    @click=\"setActiveCategory(category)\">\n\n                                    <a>{{ category.title }}</a>\n                                </li>\n\n                            </ul>\n                        </aside>\n                    </div>\n\n                    <div class=\"column is-10\">\n                        <div class=\"columns is-multiline\">\n                            <div class=\"column is-4\" v-for=\"course in courses\" v-bind:key=\"course.id\">\n\n                                <CourseItemComponent :course=\"course\" />\n\n\n                            </div>\n                            <div class=\"column is-12\">\n                                <div class=\"pagination\">\n                                    <a class=\"pagination-previous\">Previous</a>\n                                    <a class=\"pagination-next\">Next</a>\n                                    <ul class=\"pagination-list\">\n                                        <li>\n                                            <a class=\"pagination-link is-current\">1</a>\n                                        </li>\n                                    </ul>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n    </div>\n</template>\n\n<script>\nimport CourseItemComponent from '@/components/CourseItemComponent.vue';\nimport router from '@/router';\nimport axios from 'axios';\nexport default {\n    data() {\n        return {\n            courses: [],\n            categories: [],\n            activeCategory: {}\n        };\n    },\n    async mounted() {\n        console.log(\"Mounted\");\n\n        document.title = 'Courses | LMS'\n        await axios\n            .get('api/v1/courses/get-categories/')\n            .then(response => {\n                console.log(response.data)\n                this.categories = response.data\n            })\n\n        this.getCourses\n\n    },\n    components: { router, CourseItemComponent },\n    methods: {\n        setActiveCategory(category) {\n            this.activeCategory = category\n            this.getCourses()\n        },\n\n        getCourses() {\n            let url = '/api/v1/courses/'\n            if (this.activeCategory) {\n                url += '?category_id=' + this.activeCategory.id\n            }\n            axios\n                .get(url)\n                .then(response => {\n                    console.log(response.data);\n                    this.courses = response.data;\n                });\n\n        }\n    }\n}\n</script>"],"mappings":"AA6DA,OAAOA,mBAAkB,MAAO,sCAAsC;AACtE,OAAOC,MAAK,MAAO,UAAU;AAC7B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE,CAAC;IACrB,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;IAEtBC,QAAQ,CAACC,KAAI,GAAI,eAAc;IAC/B,MAAMT,KAAI,CACLU,GAAG,CAAC,gCAAgC,EACpCC,IAAI,CAACC,QAAO,IAAK;MACdN,OAAO,CAACC,GAAG,CAACK,QAAQ,CAACX,IAAI;MACzB,IAAI,CAACE,UAAS,GAAIS,QAAQ,CAACX,IAAG;IAClC,CAAC;IAEL,IAAI,CAACY,UAAS;EAElB,CAAC;EACDC,UAAU,EAAE;IAAEf,MAAM;IAAED;EAAoB,CAAC;EAC3CiB,OAAO,EAAE;IACLC,iBAAiBA,CAACC,QAAQ,EAAE;MACxB,IAAI,CAACb,cAAa,GAAIa,QAAO;MAC7B,IAAI,CAACJ,UAAU,CAAC;IACpB,CAAC;IAEDA,UAAUA,CAAA,EAAG;MACT,IAAIK,GAAE,GAAI,kBAAiB;MAC3B,IAAI,IAAI,CAACd,cAAc,EAAE;QACrBc,GAAE,IAAK,eAAc,GAAI,IAAI,CAACd,cAAc,CAACe,EAAC;MAClD;MACAnB,KAAI,CACCU,GAAG,CAACQ,GAAG,EACPP,IAAI,CAACC,QAAO,IAAK;QACdN,OAAO,CAACC,GA