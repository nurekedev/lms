{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      errors: []\n    };\n  },\n  methods: {\n    submitForm() {\n      console.log('subl');\n      this.errors = [];\n      if (this.username === '') {\n        this.errors.push(\"Username is missing\");\n      }\n      if (this.password === '') {\n        this.errors.push(\"Password is missing\");\n      }\n      if (!this.errors.length) {\n        const formData = {\n          username: this.username,\n          password: this.password\n        };\n        axios.post('/api/v1/users/', formData).then(response => {\n          this.$router.push('/login');\n        }).catch(error => {\n          if (error.response) {\n            for (const property in this.error.response.data) {\n              this.errors.push(`${property}: ${error.response.data[property]}`);\n            }\n            console.log(JSON.stringify(error.response.data));\n          } else if (error.message) {\n            this.errors.push('Something went wrong. Please tru again');\n            console.log(JSON.stringify(error));\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","errors","methods","submitForm","console","log","push","length","formData","post","then","response","$router","catch","error","property","JSON","stringify","message"],"sources":["/Users/nurzhansaktaganov/Desktop/LMS/learncom_vue/src/views/LoginView.vue"],"sourcesContent":["<template>\n    <div class=\"login\">\n        <div class=\"hero is-info\">\n            <div class=\"hero-body has-text-centered\">\n                <h1 class=\"title\"> Login</h1>\n            </div>\n        </div>\n\n        <section class=\"section\">\n            <div class=\"container\">\n                <div class=\"columns\">\n                    <div class=\"column is-4 is-offset-4\">\n\n                        <form v-on:submit.prevent=\"submitForm\">\n                            <div class=\"notification is-danger\" v-if=\"errors.length\">\n                                <p v-for=\"error in errors\" v-bind:key=\"error\">\n                                    {{ error }}\n                                </p>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>Email</label>\n                                <div class=\"control\">\n                                    <input type=\"email\" class=\"input\" v-model=\"username\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>Password</label>\n                                <div class=\"control\">\n                                    <input type=\"password\" class=\"input\" v-model=\"password\">\n                                </div>\n                            </div>\n\n\n                            <div class=\"field\">\n                                <div class=\"control\">\n                                    <button class=\"button is-dark\">Sign in</button>\n                                </div>\n                            </div>\n                        </form>\n\n                        <hr>\n                        or\n                        <router-link to=\"/signup\">Do not have an account?</router-link>\n                    </div>\n                </div>\n            </div>\n        </section>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\n\nexport default {\n\n    data() {\n        return {\n            username: '',\n            password: '',\n            errors: []\n        }\n    },\n\n    methods: {\n        submitForm() {\n            console.log('subl')\n\n            this.errors = []\n\n            if (this.username === '') {\n                this.errors.push(\"Username is missing\")\n            }\n\n            if (this.password === '') {\n                this.errors.push(\"Password is missing\")\n            }\n\n\n            if (!this.errors.length) {\n                const formData = {\n                    username: this.username,\n                    password: this.password\n                }\n\n                axios\n                    .post('/api/v1/users/', formData)\n                    .then(response => {\n                        this.$router.push('/login')\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in this.error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please tru again')\n                            console.log(JSON.stringify(error))\n                        }\n                    })\n\n            }\n\n\n\n        }\n    }\n}\n\n</script>"],"mappings":";AAqDA,OAAOA,KAAI,MAAO,OAAM;AAGxB,eAAe;EAEXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACZ;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACTC,OAAO,CAACC,GAAG,CAAC,MAAM;MAElB,IAAI,CAACJ,MAAK,GAAI,EAAC;MAEf,IAAI,IAAI,CAACF,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACE,MAAM,CAACK,IAAI,CAAC,qBAAqB;MAC1C;MAEA,IAAI,IAAI,CAACN,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACC,MAAM,CAACK,IAAI,CAAC,qBAAqB;MAC1C;MAGA,IAAI,CAAC,IAAI,CAACL,MAAM,CAACM,MAAM,EAAE;QACrB,MAAMC,QAAO,GAAI;UACbT,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB;QAEAH,KAAI,CACCY,IAAI,CAAC,gBAAgB,EAAED,QAAQ,EAC/BE,IAAI,CAACC,QAAO,IAAK;UACd,IAAI,CAACC,OAAO,CAACN,IAAI,CAAC,QAAQ;QAC9B,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;UACZ,IAAIA,KAAK,CAACH,QAAQ,EAAE;YAChB,KAAK,MAAMI,QAAO,IAA